<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader
			backgroundColor="grey">
			<m:MaterialLink ui:field="objecttitle" textColor="black"
				iconType="CREATE" iconPosition="LEFT" />
			<m:MaterialIcon ui:field="delete" iconType="CLOSE" iconPosition="RIGHT"/>
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
			<m:MaterialRow>
				<m:MaterialColumn grid="s9">
					<m:MaterialTextBox ui:field="link" text="http://"
						placeholder="Link" />
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12">
					<m:MaterialTextArea ui:field="description" text="description of link"
						placeholder="Description" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 