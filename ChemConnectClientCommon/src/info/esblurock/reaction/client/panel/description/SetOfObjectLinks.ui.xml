<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader
			backgroundColor="grey">
			<m:MaterialLink ui:field="linkheader" text="Links"
				iconType="CREATE" iconPosition="LEFT" textColor="black" />
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12">
					<m:MaterialLink ui:field="addlink" text="Add Link"
						textColor="black" iconType="CREATE" iconPosition="LEFT" />
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12">
					<m:MaterialCollapsible ui:field="links" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 