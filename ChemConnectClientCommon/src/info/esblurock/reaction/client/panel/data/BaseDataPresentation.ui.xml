<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialModal ui:field="modal" type="FIXED_FOOTER"
		dismissable="false" inDuration="500" outDuration="800">
		<m:MaterialModalContent ui:field="modalcontent">
			<g:HTMLPanel ui:field="panel">
				<m:MaterialCard ui:field="savemodal">
					<m:MaterialCardContent ui:field="savecontent">
						<m:MaterialRow>
							<m:MaterialLabel ui:field="pathtitle" />
						</m:MaterialRow>
						<m:MaterialRow>
							<m:MaterialColumn>
								<m:MaterialTextBox ui:field="prefix" />
							</m:MaterialColumn>
						</m:MaterialRow>
						<m:MaterialRow>
							<m:MaterialColumn>
								<m:MaterialTextBox ui:field="type" />
							</m:MaterialColumn>
						</m:MaterialRow>
						<m:MaterialRow>
							<m:MaterialColumn>
								<m:MaterialTextBox ui:field="postfix" />
							</m:MaterialColumn>
						</m:MaterialRow>

					</m:MaterialCardContent>
					<m:MaterialCardAction>
						<m:MaterialLink ui:field="saveclose" fontSize="1em"
							iconPosition="LEFT" iconType="CLOSE" />
						<m:MaterialLink ui:field="saveok" fontSize="1em"
							iconPosition="LEFT" iconType="SAVE" />
					</m:MaterialCardAction>

				</m:MaterialCard>
				<m:MaterialRow>
					<m:MaterialTitle ui:field="title" />
				</m:MaterialRow>
			</g:HTMLPanel>
		</m:MaterialModalContent>

		<m:MaterialModalFooter>
			<m:MaterialLink ui:field="close" fontSize="1em"
				iconPosition="LEFT" iconType="CLOSE" />
			<m:MaterialLink ui:field="basket" fontSize="1em"
				iconPosition="LEFT" iconType="SAVE" />
		</m:MaterialModalFooter>
	</m:MaterialModal>
</ui:UiBinder> 