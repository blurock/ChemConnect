<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialModal ui:field="modal" type="FIXED_FOOTER" dismissable="true" inDuration="500" outDuration="800">
		<m:MaterialModalContent ui:field="modalcontent">
			<m:MaterialTitle ui:field="title" />
		</m:MaterialModalContent>

		<m:MaterialModalFooter>
				<!--  m:MaterialLink ui:field="close" text="Top page" fontSize="1em" 
					iconPosition="LEFT" iconType="CLOSE"/ -->
				<m:MaterialLink ui:field="basket" text="Query" fontSize="1em"  
					iconPosition="LEFT" iconType="SAVE"/>
		</m:MaterialModalFooter>
	</m:MaterialModal>
</ui:UiBinder> 