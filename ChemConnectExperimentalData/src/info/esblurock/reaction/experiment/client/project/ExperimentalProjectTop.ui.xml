<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel" xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:gwt="urn:import:gwtupload.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader
			backgroundColor="grey">
			<m:MaterialLink ui:field="projectTop" textColor="black" />
			<m:MaterialLink ui:field="projectInfo" iconType="INFO"
				iconPosition="RIGHT" />
			<m:MaterialLink ui:field="deleteProject" iconType="CLOSE"
				iconPosition="RIGHT" />
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
			<m:MaterialCollapsible ui:field="projectItems">
				<m:MaterialCollapsibleItem>
					<m:MaterialCollapsibleHeader
						backgroundColor="grey">
						<m:MaterialLink ui:field="definitionTop" textColor="black" />
						<!-- m:MaterialIcon ui:field="findProject" iconType="SEARCH" iconPosition="RIGHT"/ -->
						<m:MaterialLink ui:field="definitionInfo" iconType="INFO"
							iconPosition="RIGHT" />
					</m:MaterialCollapsibleHeader>
					<m:MaterialCollapsibleBody>
						<m:MaterialCollapsible ui:field="standardItems" />
					</m:MaterialCollapsibleBody>
				</m:MaterialCollapsibleItem>
				<m:MaterialCollapsibleItem>
					<m:MaterialCollapsibleHeader
						backgroundColor="grey">
						<m:MaterialLink ui:field="experimentTop" textColor="black" />
						<m:MaterialLink ui:field="addExperimentElement"
							iconType="INPUT" iconColor="white" float="RIGHT" />
						<!-- m:MaterialIcon ui:field="experimentInfo" iconType="INFO" iconPosition="RIGHT" 
							/ -->
					</m:MaterialCollapsibleHeader>
					<m:MaterialCollapsibleBody>
						<m:MaterialCollapsible ui:field="addedItems" />
					</m:MaterialCollapsibleBody>
				</m:MaterialCollapsibleItem>
			</m:MaterialCollapsible>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 