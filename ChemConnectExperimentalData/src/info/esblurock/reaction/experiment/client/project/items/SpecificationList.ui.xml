<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel" xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:gwt="urn:import:gwtupload.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<m:MaterialRow>
			<m:MaterialColumn grid="s4">
				<MaterialTitle ui:field="title" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow>
			<m:MaterialColumn grid="s4">
				<m:MaterialLink ui:field="addProperty" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s4">
				<m:MaterialLink ui:field="addImage" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s4">
				<m:MaterialLink ui:field="addText" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow>
			<m:MaterialColumn>
				<m:MaterialCollapsible ui:field="collapsibleList" />
			</m:MaterialColumn>
		</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 