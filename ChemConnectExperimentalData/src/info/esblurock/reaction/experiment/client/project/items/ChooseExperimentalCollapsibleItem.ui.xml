<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel" xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:gwt="urn:import:gwtupload.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem backgroundColor="grey">
		<m:MaterialCollapsibleHeader
			backgroundColor="grey">
			<m:MaterialRow>
				<m:MaterialColumn>
					<m:MaterialLink ui:field="listTitle" textColor="black" />
				</m:MaterialColumn>
				<m:MaterialColumn>
					<m:MaterialLink ui:field="addItem" iconType="ADD" iconPosition="RIGHT" />
				</m:MaterialColumn>
				<m:MaterialColumn>
					<m:MaterialLink ui:field="info" iconType="INFO" iconPosition="RIGHT" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
			<m:MaterialCollapsible ui:field="itemsList" />
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 