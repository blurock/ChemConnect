<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
	<m:MaterialRow>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="label" grid="s4" />
		</m:MaterialColumn>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="name" grid="s4" />
		</m:MaterialColumn>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="parameterunits" grid="s4" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow ui:field="descriptionrow">
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="description" grid="s12" />
		</m:MaterialColumn>
	</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 