<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
	<m:MaterialRow ui:field="propertyrow">
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="parametername" grid="s4" />
		</m:MaterialColumn>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="parametervalue" grid="s4" />
		</m:MaterialColumn>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="parameterunits" grid="s4" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow ui:field="componentrow">
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="initialcomponent" grid="s2" />
		</m:MaterialColumn>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="species" grid="s4" />
		</m:MaterialColumn>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="amount" grid="s3" />
		</m:MaterialColumn>
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="units" grid="s3" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow ui:field="componentsrow">
	<m:MaterialColumn>
		<g:HTMLPanel ui:field="components"/>
	</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow ui:field="descriptionrow">
		<m:MaterialColumn>
			<m:MaterialLabel ui:field="description" grid="s12" />
		</m:MaterialColumn>
	</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 