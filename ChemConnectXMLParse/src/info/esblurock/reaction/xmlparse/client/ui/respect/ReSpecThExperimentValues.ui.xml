<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader
			backgroundColor="cyan lighten-5">
			<m:MaterialLink ui:field="description" textColor="blue" />
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
			<g:HTMLPanel>
				<g:HTMLPanel ui:field="direct">
					<m:MaterialRow>
						<m:MaterialColumn grid="s6">
							<m:MaterialTextBox ui:field="reaction" />
						</m:MaterialColumn>
						<m:MaterialColumn grid="s2">
							<m:MaterialTextBox ui:field="order" />
						</m:MaterialColumn>
						<m:MaterialColumn grid="s4">
							<m:MaterialTextBox ui:field="bulkgas" />
						</m:MaterialColumn>
					</m:MaterialRow>
				</g:HTMLPanel>
				<g:HTMLPanel ui:field="indirect">
				<m:MaterialRow ui:field="experimenttyperow">
					<m:MaterialColumn grid="s12">
						<m:MaterialTextBox ui:field="experimenttype" />
					</m:MaterialColumn>
				</m:MaterialRow>
					<m:MaterialRow  ui:field="apparatusrow">
						<m:MaterialColumn grid="s6">
							<m:MaterialTextBox ui:field="apparatuskind" />
						</m:MaterialColumn>
						<m:MaterialColumn grid="s6">
							<m:MaterialTextBox ui:field="apparatusmode" />
						</m:MaterialColumn>
					</m:MaterialRow>
					<m:MaterialRow ui:field="ignitionrow">
						<m:MaterialColumn grid="s6">
							<m:MaterialTextBox ui:field="target" />
						</m:MaterialColumn>
						<m:MaterialColumn grid="s6">
							<m:MaterialTextBox ui:field="type" />
						</m:MaterialColumn>
					</m:MaterialRow>
					<m:MaterialRow ui:field="amountrow">
						<m:MaterialColumn grid="s6">
							<m:MaterialTextBox ui:field="amount" />
						</m:MaterialColumn>
						<m:MaterialColumn grid="s6">
							<m:MaterialTextBox ui:field="unit" />
						</m:MaterialColumn>
					</m:MaterialRow>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 