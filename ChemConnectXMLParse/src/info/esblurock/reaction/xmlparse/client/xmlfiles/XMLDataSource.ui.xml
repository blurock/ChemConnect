<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel" xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:gwt="urn:import:gwtupload.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader
			backgroundColor="grey">
			<m:MaterialLink ui:field="reference" textColor="black" />
			<m:MaterialLink ui:field="retryread" iconType="AUTORENEW" iconPosition="LEFT" />
			<m:MaterialLink ui:field="delete"  iconType="DELETE" iconPosition="LEFT" />
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
		<g:HTMLPanel>
			<m:MaterialRow>
				<m:MaterialColumn grid="s4">
					<m:MaterialLink ui:field="fileName" textColor="black" />
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12">
					<g:ScrollPanel height="400px">
						<m:MaterialTextArea ui:field="fileText"
							textColor="black" backgroundColor="white" />
					</g:ScrollPanel>
				</m:MaterialColumn>
			</m:MaterialRow>
			</g:HTMLPanel>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 